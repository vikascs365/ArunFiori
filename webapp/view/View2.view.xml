<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="tcs.fin.ar.controller.View2"
	xmlns:html="http://www.w3.org/1999/xhtml">

			<Page title="Second Screen" showNavButton="true" navButtonPress="onBack">
				<content>
				  <!--  	<Button icon="sap-icon://nav-back" text="Back" press="onBack"></Button>-->
				  <ObjectHeader  title="{name}"
				                 intro="{healthBenefit}"
				                 icon="{image}"
				                 number="{price}"
				                 numberUnit="{currency}">
				      
				  </ObjectHeader>
				</content>
					<footer>
				    <Toolbar>
				          <ToolbarSpacer/>
				        <Button text="Save" type="Accept"></Button>
				        <Button text="Cancel" type="Rejected"></Button>
				    </Toolbar>
				</footer>
				
				
			</Page>

</mvc:View>